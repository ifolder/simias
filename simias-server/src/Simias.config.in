<configuration>
  <section name="Domain">
    <setting name="ServerName" value="Simias Server" />
    <setting name="AdminName" value="SimiasAdmin" />
    <setting name="AdminPassword" value="root" />
  </section>
  <section name="Authentication">
	<setting name="SimiasAuthNotRequired" value="Login.ashx, Simias.asmx:PingSimias, DomainService.asmx:GetDomainID" />
	<setting name="SimiasRequireSSL" value="no" />
  </section>
  <section name="StoreProvider">
    <setting name="Assembly" value="SimiasLib.dll" />
    <setting name="Type" value="@_StoreProviderType_@" />
  </section>
</configuration>
